<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Mal.XF.Infra.DevApp.Pages.FormattedText.FormattedStringPage"
             xmlns:controls="clr-namespace:Mal.XF.Infra.Controls;assembly=Mal.XF.Infra"
             Title="FormattedString">

    <StackLayout>
        <Label FormattedText="{Binding FormattedString}"/>

        <controls:HtmlLabel Text="{Binding HtmlString}" />
    </StackLayout>
</ContentPage>